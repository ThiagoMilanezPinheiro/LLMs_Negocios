# Exemplo de arquivo .env para o assistente LinkedIn
# NÃO COMMITAR ESTE ARQUIVO COM VALORES REAIS

# API Key do Groq (obrigatório)
GROQ_API_KEY=your-groq-api-key-here

# Configurações do modelo
GROQ_MODEL_ID=llama-3.3-70b-versatile
GROQ_TEMPERATURE=0.7

# Caminhos e configurações
CONTENT_PATH_LINKEDIN=./content_linkedin
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
FAISS_INDEX_DIR_LINKEDIN=index_faiss_linkedin

# Python buffering
PYTHONUNBUFFERED=1
